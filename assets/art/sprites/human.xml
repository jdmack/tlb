<?xml version="1.0" encoding="utf-8"?>
<sprite>
    <animation key="idle_right" time="200">
        <frame key="1" x="40" y="40" width="40" height="40" />
    </animation>
    <animation key="walk_right" time="100">
        <frame key="1" x="0"  y="40" width="40" height="40" />
        <frame key="2" x="40" y="40" width="40" height="40" />
        <frame key="3" x="80" y="40" width="40" height="40" />
        <frame key="4" x="40" y="40" width="40" height="40" />
    </animation>


    <animation key="idle_down" time="200">
        <frame key="1" x="40" y="0" width="40" height="40" />
    </animation>
    <animation key="walk_down" time="100">
        <frame key="1" x="0"  y="0"  width="40" height="40" />
        <frame key="2" x="40" y="0" width="40" height="40" />
        <frame key="3" x="80" y="0" width="40" height="40" />
        <frame key="4" x="40" y="0" width="40" height="40" />
    </animation>


    <animation key="idle_left" time="200">
        <frame key="1" x="40" y="120" width="40" height="40" />
    </animation>
    <animation key="walk_left" time="100">
        <frame key="1" x="0"  y="120" width="40" height="40" />
        <frame key="2" x="40" y="120" width="40" height="40" />
        <frame key="3" x="80" y="120" width="40" height="40" />
        <frame key="4" x="40" y="120" width="40" height="40" />
    </animation>


    <animation key="idle_up" time="200">
        <frame key="1" x="40" y="80" width="40" height="40" />
    </animation>
    <animation key="walk_up" time="100">
        <frame key="1" x="0"  y="80" width="40" height="40" />
        <frame key="2" x="40" y="80" width="40" height="40" />
        <frame key="3" x="80" y="80" width="40" height="40" />
        <frame key="4" x="40" y="80" width="40" height="40" />
    </animation>

</document>
